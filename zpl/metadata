event {
	name = zpl_tflm_begin_event;
	id = 0xA0;
	fields := struct {
		uint16_t subgraph_idx;
		uint16_t op_idx;
		ctf_bounded_string_t tag[20];
		uint32_t arena_used_bytes;
		uint32_t arena_tail_usage;
	};
};

event {
	name = zpl_tflm_end_event;
	id = 0xA1;
	fields := struct {
		uint16_t subgraph_idx;
		uint16_t op_idx;
		ctf_bounded_string_t tag[20];
		uint32_t arena_used_bytes;
		uint32_t arena_tail_usage;
	};
};

event {
	name = zpl_memory_event;
	id = 0xEE;
	fields := struct {
		ctf_bounded_string_t memory_id[10];
		uint32_t used;
		uint32_t unused;
	};
};
